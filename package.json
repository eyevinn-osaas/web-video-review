{
  "name": "web-video-review",
  "version": "0.1.0",
  "description": "Web application for reviewing broadcast video files from S3 with ffmpeg-powered streaming",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1456.0",
    "fluent-ffmpeg": "^2.1.2",
    "range-parser": "^1.2.1",
    "uuid": "^9.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.1"
  },
  "keywords": [
    "video",
    "streaming",
    "s3",
    "ffmpeg",
    "broadcast"
  ],
  "author": "Eyevinn Technology AB",
  "license": "MIT"
}
